<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Salesforce Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="30.0"/>
    <parameter name="url" value="https://www.salesforce.com/"/>

    <test name = "Salesforce Website">
        <classes>
            <class name="title.SalesforceHome"/>
            <class name="login.LogIn"/>
        </classes>
    </test>

</suite>